							{
								"referenceName": "SourceBlobDataset",
								"type": "DatasetReference",
								"parameters": {}
							}
						],
						"outputs": [
							{
								"referenceName": "OutputFileLS",
								"type": "DatasetReference",
								"parameters": {}
							}
						]
					},
